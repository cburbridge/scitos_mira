<launch>
	<arg name="machine" default="localhost" />
	<arg name="user" default="" />

	<machine name="$(arg machine)" address="$(arg machine)" env-loader="/opt/strands/strands_catkin_ws/devel/env.sh" user="$(arg user)" default="true"/>

	<node pkg="sicks300" type="sick300_driver" name="sick300" output="screen">
		<param name="devicename" value="/dev/ttyUSB0" type="string"/>
		<param name="field_of_view" value="225" type="double"/>
	</node>
</launch>
